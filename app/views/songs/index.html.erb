<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Tid</th>
      <th>Mxmid</th>
      <th>Title</th>
      <th>Artist</th>
      <th>Lyrics Hash</th>
      <th colspan="5"></th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.tid %></td>
        <td><%= song.mxmid %></td>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= song.lyrics_hash %></td>
        <td><%#= link_to 'Show', song %></td>
        <td><%#= link_to 'Edit', edit_song_path(song) %></td>
        <td><%#= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>

<%= will_paginate @songs %>

<br>

<%= link_to 'New Song', new_song_path %>
